{
    "multipart": [
        {   "when": { "facing": "north" },
            "apply": [
                { "model": "block/tube_coral_wall_fan" },
                { "model": "block/tube_coral_wall_fan2" }
            ]
        },
        {   "when": { "facing": "east" },
            "apply": [
                { "model": "block/tube_coral_wall_fan", "y": 90 },
                { "model": "block/tube_coral_wall_fan2", "y": 90 }
            ]
        },
        {   "when": { "facing": "south" },
            "apply": [
                { "model": "block/tube_coral_wall_fan", "y": 180 },
                { "model": "block/tube_coral_wall_fan2", "y": 180 }
            ]
        },
        {   "when": { "facing": "west" },
            "apply": [
                { "model": "block/tube_coral_wall_fan", "y": 270 },
                { "model": "block/tube_coral_wall_fan2", "y": 270 }
            ]
        }
    ]
}
