{
    "multipart": [
        {   "when": { "sculk_sensor_phase": "active" },
            "apply": [
                { "model": "block/sculk_sensor_active" },
                { "model": "block/sculk_sensor_active", "y": 90 },
                { "model": "block/sculk_sensor_active", "y": 180 },
                { "model": "block/sculk_sensor_active", "y": 270 }
            ]
        },
        {   "when": { "sculk_sensor_phase": "cooldown|inactive" },
            "apply": [
                { "model": "block/sculk_sensor_inactive" },
                { "model": "block/sculk_sensor_inactive", "y": 90 },
                { "model": "block/sculk_sensor_inactive", "y": 180 },
                { "model": "block/sculk_sensor_inactive", "y": 270 }
            ]
        }
    ]
}
