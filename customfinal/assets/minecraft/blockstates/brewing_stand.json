{
    "multipart": [
        {   "apply": [
                { "model": "block/brewing_stand" },
                { "model": "block/brewing_stand", "y": 90 },
                { "model": "block/brewing_stand", "y": 180 },
                { "model": "block/brewing_stand", "y": 270 }
            ]
        },
        {   "when": { "has_bottle_0": "true" },
            "apply": [
                { "model": "block/brewing_stand_bottle0" },
                { "model": "block/brewing_stand_bottle0", "y": 90 },
                { "model": "block/brewing_stand_bottle0", "y": 180 },
                { "model": "block/brewing_stand_bottle0", "y": 270 }
            ]
        },
        {   "when": { "has_bottle_1": "true" },
            "apply": [
                { "model": "block/brewing_stand_bottle1" },
                { "model": "block/brewing_stand_bottle1", "y": 90 },
                { "model": "block/brewing_stand_bottle1", "y": 180 },
                { "model": "block/brewing_stand_bottle1", "y": 270 }
            ]
        },
        {   "when": { "has_bottle_2": "true" },
            "apply": [
                { "model": "block/brewing_stand_bottle2" },
                { "model": "block/brewing_stand_bottle2", "y": 90 },
                { "model": "block/brewing_stand_bottle2", "y": 180 },
                { "model": "block/brewing_stand_bottle2", "y": 270 }
            ]
        },
        {   "when": { "has_bottle_0": "false" },
            "apply": [
                { "model": "block/brewing_stand_empty0" },
                { "model": "block/brewing_stand_empty0", "y": 90 },
                { "model": "block/brewing_stand_empty0", "y": 180 },
                { "model": "block/brewing_stand_empty0", "y": 270 }
            ]
        },
        {   "when": { "has_bottle_1": "false" },
            "apply": [
                { "model": "block/brewing_stand_empty1" },
                { "model": "block/brewing_stand_empty1", "y": 90 },
                { "model": "block/brewing_stand_empty1", "y": 180 },
                { "model": "block/brewing_stand_empty1", "y": 270 }
            ]
        },
        {   "when": { "has_bottle_2": "false" },
            "apply": [
                { "model": "block/brewing_stand_empty2" },
                { "model": "block/brewing_stand_empty2", "y": 90 },
                { "model": "block/brewing_stand_empty2", "y": 180 },
                { "model": "block/brewing_stand_empty2", "y": 270 }
            ]
        }
    ]
}
